<Window x:Class="DateTimeParseExcatDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <TextBox x:Name="tb"/>
            <StackPanel Grid.Row="1" >
                <TextBlock x:Name="tbk1"/>
                <TextBlock x:Name="tbk2"/>
                <TextBlock x:Name="tbk3"/>
            </StackPanel>
            <StackPanel Grid.Row="2" >
                <Button Click="Button_Click1" Content="parse"/>
                <Button Click="Button_Click" Content="parseExcat"/>
                <Button Click="Button_Click2" Content="convert"/>
            </StackPanel>
        </Grid>
    </Grid>
</Window>
